	<!-- UnitclassNotUnit.xml -->
    <Language_de_DE>
		<Row Tag="TXT_KEY_TP_FAITH_NEXT_PROPHET_PARAM">
			<Text>{1_Num} [ICON_PEACE] Glaube wird mindestens benötigt für Eure nächste Chance auf einen {2_UNITCLASS_PROPHET}.</Text>
		</Row>

		<Row Tag="TXT_KEY_RO_FAITH_TOOLTIP_PARAM">
