<?xml version="1.0" encoding="utf-8"?>
<GameData>
	<!-- Espionage.xml -->
    <Language_de_DE>
		<Row Tag="TXT_KEY_SPY_STATE_TERMINATED">
			<Text>Ausgelöscht!</Text>
		</Row>
	</Language_de_DE>

	<!-- GoodyHutMessages.xml -->
   	<Language_de_DE>
		<Update>
			<Where Tag="TXT_KEY_GOODY_GOLD"/>
			<Set>
				<Text>In den Ruinen findet Ihr den Schatz eines vergangenen Reiches!</Text>
			</Set>
		</Update>
		<Update>
			<Where Tag="TXT_KEY_GOODY_PROPHET_FAITH"/>
			<Set>
				<Text>Ihr habt eine geheime Religionsgemeinschaft gefunden. Der Priester sagt die Ankunft eines großen Propheten voraus!</Text>
			</Set>
		</Update>
		<Update>
			<Where Tag="TXT_KEY_GOODY_FAITH"/>
			<Set>
				<Text>Ihr habt ein altes Orakel gefunden. Die Priesterin segnet Euch mit einer göttlichen Offenbarung.</Text>
			</Set>
		</Update>
		<Update>
			<Where Tag="TXT_KEY_GOODY_CULTURE"/>
			<Set>
				<Text>Ihr habt ein kulturelles Artefakt gefunden, welches Eure Bürger beeindruckt!</Text>
			</Set>
		</Update>
		<Update>
			<Where Tag="TXT_KEY_MISC_RECEIVED_GOLD"/>
			<Set>
				<Text>[SPACE]Ihr habt {1_Num} [ICON_GOLD] Gold erhalten!</Text>
			</Set>
		</Update>
		<Row Tag="TXT_KEY_MISC_RECEIVED_CULTURE">
			<Text>[SPACE]Ihr erhaltet {1_Num} [ICON_CULTURE] Kultur!</Text>
		</Row>
		<Row Tag="TXT_KEY_MISC_RECEIVED_FAITH">
			<Text>[SPACE]Ihr erhaltet {1_Num} [ICON_PEACE] Glauben!</Text>
		</Row>
	</Language_de_DE>

	<!-- Religion.xml -->
   	<Language_de_DE>
		<Row Tag="TXT_KEY_NOTIFICATION_PANTHEON_ENHANCED_ACTIVE_PLAYER">
			<Text>Durch das Hinzufügen eines neuen Glaubenssatzes habt Ihr euer Pantheon verbessert.</Text>
		</Row>
		<Row Tag="TXT_KEY_NOTIFICATION_PANTHEON_ENHANCED">
			<Text>{1_CivName} {1: plural 1?hat sein; 2?haben ihr;} Pantheon, durch Hinzufügen eines neuen Glaubenssatzes, verbessert.</Text>
		</Row>
		<Row Tag="TXT_KEY_NOTIFICATION_PANTHEON_ENHANCED_UNKNOWN">
			<Text>Eine unbekannte Zivilisation hat ihr Pantheon, durch Hinzufügen eines neuen Glaubenssatzes, verbessert.</Text>
		</Row>
		<Row Tag="TXT_KEY_NOTIFICATION_PANTHEON_ENHANCED_S">
			<Text>Pantheon verbessert</Text>
		</Row>
	</Language_de_DE>

	<!-- SpyNames.xml -->
    <Language_de_DE>
		<Row Tag="TXT_KEY_SPY_NAME_UNKNOWN">
			<Text>Unbekannt</Text>
		</Row>
	</Language_de_DE>
    
